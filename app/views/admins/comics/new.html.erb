<div class="row">
    <div class="col-xs-9">
      <h2 class="form-header-title">漫画の新規登録</h2>
        <%= form_for(@comic, url: new_admins_comic_path) do |f| %>
         <% if @comic.errors.any? %>
          <div id="error_explanation">
           <h2><%= @comic.errors.count %>件のエラーが発生しました。</h2>
           <ul>
           <% @comic.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
     <% end %><br>
      <p>ISBNコード</p>
      <%= f.text_field :isbn_code %><br>
      <p>タイトル</p>
      <%= f.text_field :title %><br>
      <p>作者</p>
      <%= f.text_field :author %><br>
      <p>出版社</p>
      <%= f.text_field :publisher %><br>
      <p>タイトル(英語)</p>
      <%= f.text_field :title_en %><br>
      <p>作者(英語)</p>
      <%= f.text_field :author_en %><br>
      <p>出版社(英語)</p>
      <%= f.text_field :publisher_en %><br>
      <p>概要</p>
      <%= f.text_area :body, class:"form-control" %><br>
      <p>表紙</p>
      <%= f.attachment_field :comic_image %>
      <p>タグ</p>
      <!--<%= f.text_field :tag_list, value: @comic.tag_list.join(','), class: "form-control" %><br>-->
      <%= f.submit "submit", class:"btn" %>
<% end %>
</div>
</div>