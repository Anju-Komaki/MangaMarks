<div class="container">
	<div class="row">
		<% @user.followers.each do |user| %>
		<table class="table">
			<thead>
				<th><%= t('.usename') %></th>
				<th><%= t('.show') %></th>
			</thead>
		    <tbody>
		     <tr>
		       <td><%= user.username %></td>
		 	   <td><%= link_to t('.show'), user_path(user) %></td>
		     </tr>
		    </tbody>
		</table>
		<% end %>
	</div>
</div>