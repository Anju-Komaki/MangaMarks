<div class="container">
	<div class="row">
		<table class="table">
			<thead>
				<th><%= t('.username') %></th>
				<th><%= t('.show') %></th>
			</thead>
		    <tbody>
			<% @user.following.each do |user| %>
		     <tr>
		       <td><%= user.username %></td>
		 	   <td><%= link_to t('.show'), user_path(user) %></td>
		     </tr>
		    <% end %>
		    </tbody>
		</table>
	</div>
</div>