<div class="container">
	<div class="row">
	  <h1>漫画一覧</h1><br>
	  <% @comics.each do |comic| %>
	    <%= link_to comic_path(comic.id) do %>
	    <%= attachment_image_tag comic,:comic_image, :fill, 100, 100, format: 'jpeg' %>
	    <% end %><br>
	    <%= comic.title %><br>
	  <% end %>
    </div>
</div>